# User makefile generated by OptFrame MCT (Make a Compilable Thing!)

all: mctAppKP

CC=g++

mctAppKP:      mainKP.o  KPProblemInstance.o  KPEvaluator.o KPConstructiveGreedy.o KPConstructiveRandom.o  KPNSSeqBitFlip.o     
	g++ $(GCC_FLAGS) mainKP.o  KPProblemInstance.o  KPEvaluator.o KPConstructiveGreedy.o KPConstructiveRandom.o KPNSSeqBitFlip.o  -o app_KP

mainKP.o: mainKP.cpp  src/Representation.h  src/Solution.h 
	g++ -c $(GCC_FLAGS) mainKP.cpp -o mainKP.o
	
KPProblemInstance.o: src/ProblemInstance.cpp src/ProblemInstance.h 
	g++ -c $(GCC_FLAGS) src/ProblemInstance.cpp -o KPProblemInstance.o
	
KPEvaluator.o: src/Evaluator.cpp src/Evaluator.h
	g++ -c $(GCC_FLAGS) src/Evaluator.cpp -o KPEvaluator.o

KPConstructiveRandom.o: src/ConstructiveRandom.cpp src/ConstructiveRandom.h
	g++ -c $(GCC_FLAGS) src/ConstructiveRandom.cpp -o KPConstructiveRandom.o

KPConstructiveGreedy.o: src/ConstructiveGreedy.cpp src/ConstructiveGreedy.h
	g++ -c $(GCC_FLAGS) src/ConstructiveGreedy.cpp -o KPConstructiveGreedy.o

KPNSSeqBitFlip.o: src/NSSeqBitFlip.cpp src/NSSeqBitFlip.h
	g++ -c $(GCC_FLAGS) src/NSSeqBitFlip.cpp -o KPNSSeqBitFlip.o
	
#KPprintable.o: ../OptFrame/Util/printable.cpp
#	g++ -c $(GCC_FLAGS) ../OptFrame/Util/printable.cpp -o KPprintable.o


GCC_FLAGS= -I../../src  -g -Wall -O0 --std=c++17 -fconcepts #-fno-exceptions -fno-rtti #TODO: disable exceptions

#.PHONY: all #clean
	
clean: 
	rm -f app_KP
	rm -f KP*.o
