# User makefile generated by OptFrame MCT (Make a Compilable Thing!)

all: mctAppKP2

mctAppKP2: mainKP2.o  KP2ProblemInstance.o  KP2Evaluator.o  KP2InitialPopulation.o  KP2Crossover.o KP2Mutation.o
	g++ $(GCC_FLAGS) mainKP2.o  KP2ProblemInstance.o  KP2Evaluator.o KP2InitialPopulation.o KP2Crossover.o KP2Mutation.o -o app_KP2

mainKP2.o: mainKP2.cpp KP2/Representation.h KP2/ProblemInstance.h KP2/Evaluator.h KP2/InitialPopulation.h KP2/Mutation.h KP2/Crossover.h
	g++ -c $(GCC_FLAGS) mainKP2.cpp -o mainKP2.o
	
KP2ProblemInstance.o: KP2/ProblemInstance.cpp KP2/ProblemInstance.h 
	g++ -c $(GCC_FLAGS) KP2/ProblemInstance.cpp -o KP2ProblemInstance.o
	
KP2Evaluator.o: KP2/Evaluator.cpp KP2/Evaluator.h KP2/ProblemInstance.h KP2/Representation.h
	g++ -c $(GCC_FLAGS) KP2/Evaluator.cpp -o KP2Evaluator.o

KP2InitialPopulation.o: KP2/InitialPopulation.cpp KP2/InitialPopulation.h KP2/ProblemInstance.h KP2/Representation.h 
	g++ -c $(GCC_FLAGS) KP2/InitialPopulation.cpp -o KP2InitialPopulation.o

KP2Crossover.o: KP2/Crossover.cpp KP2/Crossover.h KP2/ProblemInstance.h KP2/Representation.h
	g++ -c $(GCC_FLAGS) KP2/Crossover.cpp -o KP2Crossover.o

KP2Mutation.o: KP2/Mutation.cpp KP2/Mutation.h KP2/ProblemInstance.h KP2/Representation.h
	g++ -c $(GCC_FLAGS) KP2/Mutation.cpp -o KP2Mutation.o

GCC_FLAGS= -O3 --std=c++17 -fconcepts  -DNDEBUG #-DOPTFRAME_DEBUG

examples:
	make -C ../Examples/
	
clean: 
	make clean -C ../Examples/
	rm -f app_*
	rm -f *.o

run:
	./app_KP2 < ./KP2/instances/f1_l-d_kp_10_269 > ./f1_l-d_kp_10_269.out 2> ./f1_l-d_kp_10_269.err
	./app_KP2 < ./KP2/instances/f2_l-d_kp_20_878 > ./f2_l-d_kp_20_878.out 2> ./f2_l-d_kp_20_878.err
	./app_KP2 < ./KP2/instances/knapPI_1_100_1000_1 > ./knapPI_1_100_1000_1.out 2> ./knapPI_1_100_1000_1.err